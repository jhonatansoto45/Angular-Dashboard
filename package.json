    "@angular/platform-browser-dynamic": "^15.0.0",
    "@angular/router": "^15.0.0",
    "chart.js": "^4.2.1",
    "husky": "^8.0.3",
    "ngx-drag-scroll": "^15.0.0",
    "ngx-hide-on-scroll": "^0.2.1",
    "primeng": "^15.4.1",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.12.0"
  },
  "devDependencies": {
